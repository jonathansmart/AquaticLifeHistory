year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(
  bibtype = "Manual",
  title = paste("{AquaticLifeHistory}: Fisheries life history analysis using contemporary methods"),
  author = c(person("Jonathan", "Smart")),
  year = year,
  note = note,
  url = "https://github.com/jonathansmart/AquaticLifeHistory"
)
